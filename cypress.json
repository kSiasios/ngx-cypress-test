{
    "baseUrl": "http://localhost:4200",
    "ignoreTestFiles": [
        "**/1-getting-started/*",
        "**/2-advanced-examples/*",
        "**/__snapshots__/*",
        "**/__image_snapshots__/*"
    ],
    "viewportWidth": 1920,
    "viewportHeight": 1080,
    "env": {
        "cypress-plugin-snapshots": {
            "imageConfig": {
                "theshold": 0.01
            }
        }
    }
}